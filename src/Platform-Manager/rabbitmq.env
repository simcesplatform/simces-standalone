RABBITMQ_HOST=simces_rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_LOGIN=
RABBITMQ_PASSWORD=
RABBITMQ_SSL=false
RABBITMQ_SSL_VERSION=PROTOCOL_TLS

# For the platform manager these exchange parameters define the management exchange
RABBITMQ_EXCHANGE=procem-management
RABBITMQ_EXCHANGE_AUTODELETE=false
RABBITMQ_EXCHANGE_DURABLE=true

# The prefix for all simulation specific exchanges
RABBITMQ_EXCHANGE_PREFIX=procem.
